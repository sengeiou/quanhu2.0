        android:textColor="@color/font_gray_m"/>


    <ListView
        android:id="@+id/id_homefrg_listview"
        style="@style/DefaultLvStyle"
        android:background="@color/color_F6F8F8"
        android:clipToPadding="false"
        android:divider="@color/color_F4F4F4"
        />


</LinearLayout>
