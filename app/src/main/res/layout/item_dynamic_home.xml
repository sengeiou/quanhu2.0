<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:paddingBottom="@dimen/px40"
    android:paddingLeft="@dimen/px44"
    android:paddingRight="@dimen/px44">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px58"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/civ_head"
            android:layout_width="@dimen/px98"
            android:layout_height="@dimen/px98"
            android:src="@drawable/ic_default_head_new" />

        <com.rz.circled.widget.CircleImageView
            android:id="@+id/civ_superV"
            android:layout_width="@dimen/px38"
            android:layout_height="@dimen/px38"
            android:layout_gravity="top"
            android:layout_marginLeft="-10dp"
            android:src="@drawable/famous_icon"
            android:visibility="gone" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginLeft="@dimen/px30">

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="name"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_44" />

            <TextView
                android:id="@+id/tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_gravity="bottom"
                tools:text="@string/two_hour_later"
                android:visibility="gone"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_34" />
        </RelativeLayout>

        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/tv_circle_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="@string/default_circle"
            android:textColor="@color/color_666666"
            android:visibility="gone"
            android:textSize="@dimen/font_34" />
    </LinearLayout>

