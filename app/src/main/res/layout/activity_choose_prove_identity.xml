<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingLeft="@dimen/app_base_padding"
    android:paddingRight="@dimen/app_base_padding"
    tools:context="com.rz.circled.ui.activity.ChooseProveIdentityActivity">

    <ImageView
        android:id="@+id/iv_choose_prove_oneself"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/px55"
        android:layout_marginTop="@dimen/px55"
        android:src="@mipmap/bg_prove_identity_oneself"/>

    <ImageView
        android:id="@+id/iv_choose_prove_agency"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/bg_prove_identity_agency"/>

</LinearLayout>
